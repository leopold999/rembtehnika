extends ../../../layout/article_page_layout.pug

block variables
  - var title = 'Коды ошибок посудомоечных машин Candy (Канди)'
  - var content = "Расшифровка ошибок посудомоечных машин Candy (Канди)"
  - var pathToImgArticles = domen + '/assets/img/pictures/errors-img/'

block content
  +bread-crumbs({ 
    'Главная': '/',
    "Коды ошибок бытовой техники": "/errors/",
    "Коды ошибок посудомоечных машин": "/errors/kody-oshibok-posudomoechnyh-mashin/",
    "Коды ошибок посудомоечных машин Candy (Канди)" : ""
  })
  .errors-page.article-page-wrapper(itemscope itemtype="https://schema.org/Article")
    H1(itemprop="name")= title
    +pageBanner-1({
      title: 'Нужна консультация?',
      text: 'Мы ответим на все Ваши вопросы и рассчитаем стоимость ремонта',
      buttonText: 'Заказать звонок',
      pathToImg: domen + '/assets/img/pictures/banner-img-016.webp'
    })
    .page-text(itemprop="articleBody")
      .table-container_scroll 
        table(border='1')
          tbody
            tr
              th
                p Код ошибки
              th
                p Описание ошибки
            tr
              td
                p Е2
              td
                p Ошибка залива воды
            tr
              td
                p ЕЗ
              td
                p Отсутствует слив воды
            tr
              td
                p Е4
              td
                p Сработал АкваСтоп
            tr
              td
                p Е5
              td
                p Ошибка датчика температуры NTC
            tr
              td
                p Е6
              td
                p Ошибка памяти Eeprom
            tr
              td
                p Е7
              td
                p Отсутствует слив воды.
                p Нет связи между силовым модулем и электронной платой моторного насоса
            tr
              td
                p Е8
              td
                p Нет нагрева
            tr
              td
                p Е9
              td
                p Проблемы с датчиком мутности
            tr
              td
                p Е10
                p Еа
              td
                p Дефект селектора программ
            tr
              td
                p Е12
                p Ес
              td
                p Заклинил ротор помпы
            tr
              td
                p Е14
                p Ее
              td
                p Ошибка в одном или нескольких компонентах, связанных с нагревом воды
            tr
              td
                p Е15
                p Ef
              td
                p Проблема со счетчиком литров
            tr
              td
                p Е16
                p Eh
              td
                p Прессостат находится в положении "полный бак", и реле обогревателя разомкнуто
            tr
              td
                p Е17
                p Ei
              td
                p Неполадки с реле нагревательного элемента на силовом модуле
            tr
              td
                p Е18
                p El
              td
                p Неправильный уровень воды в баке
      H2 Индикация ошибок
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-1.jpg')
      .table-container_scroll 
        table(border='1')
          tbody
            tr
              th
                p Код
              th
                p Мигает
                p индикатор
              th
                p Описание
            tr
              td
                p Е1
              td
                p 1 - 2
              td
                p Нет залива воды
            tr
              td
                p ЕЗ
              td
                p 1 - 2 - 3
              td
                p Отсутствует подогрев воды
            tr
              td
                p Е4
              td
                p 1 - 2 - 3 - 4
              td
                p Сработал АкваСтоп
            tr
              td
                p Еб
                p Еб
              td
                p 1-3
              td
                p Ошибка NTC датчика
            tr
              td
                p Е7
              td
                p 1-4
              td
                p NTC датчик в КЗ
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-2.jpg')
      .table-container_scroll           
        table(border='1')
          tbody
            tr
              th
                p Код
              th
                p Мигает
                p индикатор
              td
                p Описание
            tr
              td
                p El
              td
                p 4
              td
                p Нет залива воды
            tr
              td
                p E3
              td
                p 3-4
              td
                p Отсутствует подогрев воды
            tr
              td
                p E4
              td
                p 2
              td
                p АкваСтоп
            tr
              td
                p E6
              td
                p 2-3
              td
                p Ошибка NTC датчика
            tr
              td
                p E7
              td
                p 2-3-4
              td
                p NTC датчик в КЗ
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-3.jpg')
      p Количество мигание всех индикаторов соответствует номеру ошибки. Например: все индикаторы моргают 3 раза - ошибка ЕЗ.
      .table-container_scroll       
        table(border='1')
          tbody
            tr
              th
                p Код ошибки
              th
                p Описание ошибки
            tr
              td
                p Е2
              td
                p Нет залива воды
            tr
              td
                p ЕЗ
              td
                p Ошибка слива воды
            tr
              td
                p Е5
              td
                p Ошибка датчика NTC
            tr
              td
                p Е8
              td
                p Нет нагрева воды
            tr
              td
                p Е15
                p Ef
              td
                p Ошибка прессостата
        
      h2 Нет залива воды
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-4.jpg')
      h2 Сработал АкваСтоп
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-5.jpg')
      h2 Ошибка NTC датчика
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-6.jpg')
      h2 Ошибка NTC датчика
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-7.jpg')
      h2 Ошибка нагрева
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-8.jpg')
      img(alt='коды ошибок посудомоечных машин Candy' src=pathToImgArticles + 'pm_candy-9.jpg')
      .table-container_scroll   
        table(border='1')
          tbody
            tr
              th
                p LEDS ошибка
              th
                p Описание ошибки
            tr
              td
                p Р1 +Р2
              td
                p Нет залива воды
            tr
              td
                p Р1 + Р2 + РЗ
              td
                p Ошибка нагрева воды
            tr
              td
                p PI + Р2 + РЗ + Р4
              td
                p сработал АкваСтоп
            tr
              td
                p Р1 +Р4
              td
                p Датчик NTC в КЗ
            tr
              td
                p Р1 + РЗ
              td
                p Датчик NTC не подключен
        



  .calc-banner.page-banner
    .page-banner-img
      img(class='page-banner-img__pct' src= domen + '/assets/img/pictures/calculator.webp' alt="качественный ремонт посудомоечных машин с гарантией")
    .page-banner-content
      .page-banner-content__title 
        p 100% качественный ремонт посудомоечных машин на дому!
      .page-banner-content__text 
        p Свяжитесь с нами по телефонам  
        a(href=telNumber_1_int)= telNumber_1 
        |  или 
        a(href=telNumber_2_int)= telNumber_2 
        |  и мы проконсультируем Вас о стоимости ремонта Вашей техники
      .page-banner-content__text Также Вы можете оформить on-line заявку или заказать обратный звонок
      .page-banner-content-buttons
        .page-banner-button.button-order 
          | Оформить заявку
          br
          | on-line
        .page-banner-button.button-callback Заказать звонок

