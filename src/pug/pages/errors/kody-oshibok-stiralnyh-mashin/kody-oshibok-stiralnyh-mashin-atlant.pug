                p F3
              td
                p Неисправен ТЭН
            tr
              td
                p 2
              td
                p F4
              td
                p Нет слива воды
            tr
              td
                p 2-4
              td
                p F5
              td
                p Ошибка при заливе воды
            tr
              td
                p 2-3
              td
                p F6
              td
                p Неисправность электродвигатег
            tr
