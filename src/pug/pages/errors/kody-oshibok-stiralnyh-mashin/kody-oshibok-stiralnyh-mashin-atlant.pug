extends ../../../layout/article_page_layout.pug

block variables
  - var title = 'Коды ошибок стиральных машин Atlant (Атлант)'
  - var content = "Расшифровка ошибок стиральных машин Atlant (Атлант)'"
  - var pathToImgArticles = domen + '/assets/img/pictures/errors-img/'

block content
  +bread-crumbs({ 
    'Главная': '/',
    "Коды ошибок бытовой техники": "/errors/",
    "Коды ошибок стиральных машин": "/errors/kody-oshibok-stiralnyh-mashin/",
    "Коды ошибок стиральных машин Atlant (Атлант)" : ""
  })
  .errors-page.article-page-wrapper(itemscope itemtype="https://schema.org/Article")
    H1(itemprop="name")= title
    +pageBanner-1({
      title: 'Нужна консультация?',
      text: 'Мы ответим на все Ваши вопросы и рассчитаем стоимость ремонта',
      buttonText: 'Заказать звонок',
      pathToImg: domen + '/assets/img/pictures/banner-img-011.webp'
    })
    .page-text(itemprop="articleBody")
      .table-container_scroll 
        table(border='1')
          tbody
            tr
              th
                p Код ошибки
              th
                p Описание ошибки
            tr
              td
                p F2
              td
                p Неисправен температурный датчик
            tr
              td
                p F3
              td
                p Неисправен ТЭН
            tr
              td
                p F4
              td
                p Неисправен электронасос
            tr
              td
                p F5
              td
                p Ошибка при заливе воды
            tr
              td
                p F6, F9 или F12
              td
                p Неисправность электродвигателя
            tr
              td
                p F7
              td
                p Несоответствие частоты электросети
            tr
              td
                p F8
              td
                p Датчик реле уровня — перелив
            tr
              td
                p F10
              td
                p Электрозамок блокировки дверцы или модуль управления
            tr
              td
                p F11 или (DOOR)
              td
                p Ошибка устройства блокировки люка
            tr
              td
                p F13, F14
              td
                p Неисправен модуль управления или модуль интерфейса
            tr
              td
                p F15
              td
                p Протечка воды в поддон
            tr
              td
                p SEL
              td
                p Ошибка селектора модуля интерфейса
            tr
              td
                p None
              td
                p Избыточное пенообразование
      h1
  
        | Считывание ошибки led индикаторов
      img(alt='коды ошибок стиральных машин Atlant' src= pathToImgArticles + '10423b303d42-1.jpg')
      img(alt='коды ошибок стиральных машин Atlant' src= pathToImgArticles + '10423b303d42-2.jpg')
      .table-container_scroll 
        table(border='1')
          tbody
            tr
              th
                p Моргают
                p индикаторы
              th
