extends ../../../layout/article_page_layout.pug

block variables
  - var title = 'Коды ошибок стиральных машин daewoo (ДЭУ)'
  - var content = "Расшифровка ошибок стиральных машин daewoo (ДЭУ)'"
  - var pathToImgArticles = domen + '/assets/img/pictures/errors-img/'

block content
  +bread-crumbs({ 
    'Главная': '/',
    "Коды ошибок бытовой техники": "/errors/",
    "Коды ошибок стиральных машин": "/errors/kody-oshibok-stiralnyh-mashin/",
    "Коды ошибок стиральных машин daewoo (ДЭУ)" : ""
  })
  .errors-page.article-page-wrapper(itemscope itemtype="https://schema.org/Article")
    h1(itemprop="name")= title
    +pageBanner-1({
      title: 'Нужна консультация?',
      text: 'Мы ответим на все Ваши вопросы и рассчитаем стоимость ремонта',
      buttonText: 'Заказать звонок',
      pathToImg: domen + '/assets/img/pictures/banner-img-014.webp'
    })
    .page-text(itemprop="articleBody")
      .table-container_scroll 
        table(border='1')
          tbody
            tr
              th
                p Код ошибки
              th
                p Описание ошибки
            tr
              td
                p ОЕ
              td
                p Не происходит слив воды
            tr
              td
                p IE
              td
                p Ошибка залива воды
            tr
              td
                p UE
              td
                p Отсутствует баланс в баке
            tr
              td
                p LE
              td
                p Ошибка блокировки дверцы люка
            tr
              td
                p Е2
              td
                p Перелив воды
            tr
              td
                p Е4
              td
                p Самослив
            tr
              td
                p Е5
              td
                p Перегрузка высоковольтной цепи, барабан заклинен
            tr
              td
                p Е6
              td
                p Перегрузка цепи мотора, мотор или барабан заклинен
            tr
              td
                p Е8
              td
                p Неисправность мотора. Ошибка датчика загрузки
            tr
              td
                p Е9
              td
                p Ошибка датчика уровня воды
            tr
              td
                p Н2
              td
                p NTC обрыв
            tr
              td
                p Н4
              td
                p NTC короткий или перегрев
            tr
              td
                p Н5
              td
                p Слишком горячая вода. Температура выше чем 45' на деликатной или шерсти
            tr
              td
                p Н6
              td
                p Ошибка в работе ТЭНа
            tr
              td
                p Н8
              td
                p Перегрев. Нагрев без воды
            tr
              td
                p lb
              td
                p Дефект 1GBT
            tr
              td
                p PFE
              td
                p Засор сливного фильтра
      h2 Коды ошибок Daewoo DWD-M8011 и аналогичные
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-1.jpg')
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-2.jpg')
      h3 ОЕ - Нет слива воды
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-3.jpg')
      h3 Е7 - Неисправность мотора

      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-4.jpg')
      h3 Е2 - Перелив воды
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-5.jpg')
      h3 Н2 - NTC обрыв
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-6.jpg')
      h3 UE - Дисбаланс
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-7.jpg')
      h3 LE - Дверь не закрыта
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-8.jpg')
      h3 Ошибки Daewoo DWD-MH121KK и аналоги
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-9.jpg')
      img(alt='коды ошибок стиральных машин daewoo' src=pathToImgArticles + 'daewoo-10.jpg')



  .calc-banner.page-banner
    .page-banner-img
      img(class='page-banner-img__pct' src= domen + '/assets/img/pictures/calculator.webp' alt="100% качественный ремонт стиральных машин")
    .page-banner-content
      .page-banner-content__title 
        p 100% качество и гарантия на ремонт стиральных машин!
      .page-banner-content__text 
        p Свяжитесь с нами по телефонам  
        a(href=telNumber_1_int)= telNumber_1 
        |  или 
        a(href=telNumber_2_int)= telNumber_2 
        |  и мы проконсультируем Вас о стоимости ремонта Вашей техники
      .page-banner-content__text Также Вы можете оформить on-line заявку или заказать обратный звонок
      .page-banner-content-buttons
        .page-banner-button.button-order 
          | Оформить заявку
          br
          | on-line
        .page-banner-button.button-callback Заказать звонок

